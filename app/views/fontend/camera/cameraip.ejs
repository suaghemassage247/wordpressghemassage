<div class="laptop">
  <div class="laptop-left">
    <div class="danhmuc-laptop">
      <h3>Danh Mục</h3>
    </div>
    <div class="menu-laptop">
      <ul class="list-item">
        <li class="">
          <a href="/camera-quan-sat-trong-nha">Camera Quan Sát Trong Nhà</a>
        </li>
        <li class="">
          <a href="/camera-quan-sat-ngoai-troi">Camera Quan Sát Ngoài Trời</a>
        </li>
        <li class="">
          <a href="/camera-ip">Camera IP Wifi Dùng Cho Gia Đình</a>
        </li>
        <li class="">
          <a href="/dau-ghi-hinh">Đầu Ghi Hình Camera</a>
        </li>

      </ul>
    </div>
  </div>
  <div class="laptop-right">
      <div class="product">
        <div class="title-product">
          <h2>Camera IP Wifi</h2>
        </div>
        <div class="main-product">
          <ul class="wapper-item">
              <% for(var i=0;i<laptop.count;i++){ %>
            <li class="item-product">
              <div class="image-product">

                <img src="/assest/image/<%=laptop.laptops[i].imagedefault[0].originalname %> " alt="<%=laptop.laptops[i].imagedefault %> ">
                  <div class="tinhnang">

                      <%-laptop.laptops[i].tinhnang %>


                  </div>
              </div>
              <div class="wapper-info-product">
                <div class="info-product">
                  <div class="name-product">
                    <a href="#"><%=laptop.laptops[i].name %></a>
                  </div>
                  <div class="price-product">
                     <span  class="commas"><%=laptop.laptops[i].price %></span>
                     <sup> đ</sup>
                  </div>
                </div>
                <div class="button-view">
                    <a href="/<%=laptop.laptops[i].nameseo %>">Xem Ngay</a>
                </div>

            </li>
        <% } %>


          </ul>
        </div>
      </div>
  </div>

</div>
