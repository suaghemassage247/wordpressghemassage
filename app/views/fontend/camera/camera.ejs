<div class="breadcrumb">
      <ol>
            <li class="breadcrumb-item"><a href="/"><i class="fa fa-home"></i> Trang chủ</a></li>
              <li class="breadcrumb-item" active="" aria-current="page"> Lắp đặt Camera </li>
      </ol>
</div>
<div class="panel-tintuc">
  <div class="tab-tintuc">
    <ul class="wapper-tintuc">
      <li class="item-tab active">
        <span>Tư vấn chọn mua</span>
        <ul>
          <li class="item-tintuc ">
            <div class="owl-slide-tintuc">
            <% for(var i=0;i<poststuvan.count;i++){ %>
              <div class="owl-item">
                  <div class="detail-tintuc">
                    <div class="image-tintuc">
                        <img src="/assest/image/<%=poststuvan.post[i].image%>" alt="">
                    </div>

                    <div class="content-tintuc">
                        <div class="title-tintuc">
                          <a href="/tin-tuc/<%=poststuvan.post[i].titleseo%>" title=""> <%=poststuvan.post[i].title%></a>
                        </div>
                        <div class="mota-tintuc">
                            <%-poststuvan.post[i].shortdescription%>
                        </div>
                    </div>
                  </div>
              </div>
            <%} %>


            </div>
          </li>
        </ul>
      </li>
      <li class="item-tab ">
        <span>Hướng dẫn kỹ thuật</span>
        <ul>
          <li class="item-tintuc ">
            <div class="owl-slide-tintuc">
              <% for(var i=0;i<postshuongdan.count;i++){ %>
                <div class="owl-item">
                    <div class="detail-tintuc">
                      <div class="image-tintuc">
                          <img src="/assest/image/<%=postshuongdan.post[i].image%>" alt="">
                      </div>
                      <div class="content-tintuc">
                          <div class="title-tintuc">
                            <a href="/tin-tuc/<%=postshuongdan.post[i].titleseo%>" title=""> <%=postshuongdan.post[i].title%></a>
                          </div>
                          <div class="mota-tintuc">
                              <%-postshuongdan.post[i].shortdescription%>
                          </div>
                      </div>
                    </div>
                </div>
              <%} %>
              </div>

            </div>
          </li>
        </ul>
      </li>
    </ul>
  </div>



<div class="laptop">
  <div class="laptop-left">
    <div class="danhmuc-laptop">
      <h3>Danh Mục</h3>
    </div>
    <div class="menu-laptop">
      <ul class="list-item">
        <li class="">
          <a href="/camera-quan-sat-trong-nha">Camera Quan Sát Trong Nhà</a>
        </li>
        <li class="">
          <a href="/camera-quan-sat-ngoai-troi">Camera Quan Sát Ngoài Trời</a>
        </li>
        <li class="">
          <a href="/camera-ip">Camera IP Wifi Dùng Cho Gia Đình</a>
        </li>
        <li class="">
          <a href="/dau-ghi-hinh">Đầu Ghi Hình Camera</a>
        </li>

      </ul>
    </div>
  </div>
  <div class="laptop-right">
      <div class="product">
        <div class="title-product">
          <h2>Camera Được Tìm Kiếm Nhiều Nhất</h2>
        </div>
        <div class="main-product">
          <ul class="wapper-item">
              <% for(var i=0;i<cameraall.count;i++){ %>
            <li class="item-product">

              <div class="image-product">
                <img src="/assest/image/<%=cameraall.cameras[i].imagedefault[0].originalname %> " alt="<%=cameraall.cameras[i].imagedefault %> ">
                  <div class="tinhnang"><%-cameraall.cameras[i].tinhnang%></div>
              </div>
              <div class="wapper-info-product">
                <div class="info-product">
                  <div class="name-product">
                    <a href="#"><%=cameraall.cameras[i].name %></a>
                  </div>
                  <div class="price-product">
                     <span class="commas"><%=cameraall.cameras[i].price %></span>
                    <sup > đ</sup>
                  </div>
                </div>
                <div class="button-view">
                    <a href="/<%=cameraall.cameras[i].nameseo %>">Xem Ngay</a>
                </div>
              </div>
            </li>
        <% } %>


          </ul>
        </div>
      </div>
  </div>

</div>
